		"devdoc": {
			"details": "Contract for managing study groups and rewarding study sessions",
			"kind": "dev",
			"methods": {
				"canVerifyRecentSession(address)": {
					"details": "Check if a member can receive verification for recent study session",
					"params": {
						"member": "Address of the member"
					},
					"returns": {
						"canVerify": "Whether the member can be verified",
						"sessionIndex": "Index of the session to verify"
					}
				},
				"constructor": {
					"details": "Constructor to create a new study group",
					"params": {
						"_description": "Description of the study group",
						"_minStakeAmount": "Minimum amount required to join the group",
						"_name": "Name of the study group",
						"_studyTokenAddress": "Address of the StudyToken contract"
					}
				},
				"getMemberCount()": {
					"details": "Get total number of members in the group"
				},
				"getMemberStudySessions(address)": {
					"details": "Get study sessions for a specific member",
					"params": {
						"member": "Address of the member"
					}
				},
				"joinGroup(uint256)": {
					"details": "Join a study group by staking tokens",
					"params": {
						"stakeAmount": "Amount of tokens to stake (must be >= minStakeAmount)"
					}
				},
				"leaveGroup()": {
					"details": "Leave the study group and receive staked tokens back"
				},
				"logStudySession(uint256,string)": {
					"details": "Log a new study session",
					"params": {
						"duration": "Duration of study in minutes",
						"studyTopic": "Topic that was studied"
					}
				},
				"verifyStudySession(address,uint256)": {
					"details": "Verify a study session and reward the member",
					"params": {
						"member": "Address of the member",
						"sessionIndex": "Index of the session to verify"
					}
				}
			},
